{
  "name": "hdb-resale",
  "version": "2.0.0",
  "description": "My Web Visualization Project (rewritten in React)",
  "main": "server_dist/server.js",
  "scripts": {
    "start": "node server_dist/server.js",
    "start:dev": "babel-watch server_src/server.js",
    "build": "npm run clean && npm run build-server && npm run build-js",
    "clean": "rm -rf server_dist",
    "build-server": "babel server_src --out-dir server_dist",
    "build-js": "webpack --progress --colors --config ./webpack.prod.config.js",
    "deploy": "npm run docker-build && npm run docker-push && oc import-image yongjun21/hdb-resale:latest",
    "docker-build": "npm run build && docker build -t yongjun21/hdb-resale:latest .",
    "docker-push": "docker push yongjun21/hdb-resale:latest",
    "setup-cron": "oc create -f server_src/worker/secret.yaml && oc create -f server_src/worker/scheduledJob.yaml",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yongjun21/hdb-resale.git"
  },
  "author": "Yong Jun",
  "contributors": [
    "Albert Salim",
    "Thong Yong Jun",
    "Yap Hong Kheng"
  ],
  "bugs": {
    "url": "https://github.com/yongjun21/hdb-resale/issues"
  },
  "homepage": "https://github.com/yongjun21/hdb-resale#readme",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "express-history-api-fallback": "^2.0.0",
    "jStat": "^1.5.3",
    "lodash": "^4.16.4",
    "loess": "^1.3.4",
    "mathjs": "^3.6.0",
    "mongoose": "^4.6.5",
    "node-fetch": "^1.6.3",
    "pouchdb": "^6.0.7",
    "proj4": "^2.3.15",
    "query-string": "^4.3.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router-dom": "^4.0.0",
    "sg-heatmap": "^2.4.6",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-watch": "^2.0.3",
    "css-loader": "^0.25.0",
    "postcss-cssnext": "^2.8.0",
    "postcss-loader": "^0.13.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "standard": "^8.6.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2"
  },
  "standard": {
    "global": [
      "Plotly",
      "google"
    ]
  }
}
